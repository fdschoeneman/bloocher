.background-white.footer.visible-phone
  .container
    %p.pull-right
      %a#toplink{:href => "#"} Back to top
    %ul#footer-nav.nav.visible-phone
      %hr/
      %h3.centered Menu
      = render 'boilerplate_links'
      = render 'nav_links'
.navbar.navbar-fixed-bottom.hidden-phone
  .navbar-inner
    .container
      %ul.nav.pull-right
        = nav_link_to "home", root_url
        = render "boilerplate_links"
- unless user_signed_in?
  = render 'signup_modal'
  = render 'login_modal'
